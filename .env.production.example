# Production Environment Variables for hamkasb-ai.uz
# Copy this file to .env.production and fill in the actual values

# Database (Production PostgreSQL)
DATABASE_URL=postgresql://username:password@host:5432/hamkasb_production

# Qdrant Vector Database
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key

# OpenAI API
OPENAI_API_KEY=sk-proj-your-openai-api-key

# Vercel Blob Storage
BLOB_READ_WRITE_TOKEN=vercel_blob_rw_your-token

# Application URLs (Production)
NEXT_PUBLIC_APP_URL=http://hamkasb-ai.uz
NEXT_PUBLIC_API_URL=http://hamkasb-ai.uz/api

# Node Environment
NODE_ENV=production
